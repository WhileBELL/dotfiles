shell /usr/bin/zsh

font_family      JetBrains Mono Nerd Font
bold_font        auto
italic_font      auto
bold_italic_font auto
font_size 12.0

window_margin_width 10.0
single_window_margin_width 0

background_opacity 0.5

map f1 new_window_with_cwd

# BEGIN_KITTY_THEME
# Solarized Light
include current-theme.conf
# END_KITTY_THEME
